class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) //{ {3,2,4}  t=6
        map<int, int>mpp; //{3 ,2, 4}
        for(int i =0; i<nums.size(); i++) { 
            int num= nums[i]; //num=4
            int needed_more= target-num; //6-4=2
            if(mpp.find(needed_more)!=mpp.end()){ 
                return {mpp[needed_more],i}; // {2 ,1}
            }
            mpp[num]=i;// {2->1}
        }
        return{-1, -1};

    }
};

